z<div class="layout-container">

    <header role="banner">
        {{ page.header }}
    </header>

    {% if page.banner_top %}
        <div class="layout-banner-top" role="banner">
            {{ page.banner_top }}
        </div>
    {% endif %}

    {{ page.help }}

    <main role="main">
        <a id="main-content" tabindex="-1"></a>{# link is in html.html.twig #}
        <div class="layout-content">
            {{ page.content }}
            {{ page.content_bottom }}
        </div>

    </main>

    {% if page.banner_bottom %}
        <div class="layout-banner-top" role="banner">
            {{ page.banner_top }}
        </div>
    {% endif %}

    {% if page.banner_bottom %}
        <div class="layout-banner-top" role="banner">
            {{ page.banner_top }}
        </div>
    {% endif %}

    {% if page.footer %}
        <footer role="contentinfo">
            {{ page.footer }}
        </footer>
    {% endif %}

</div>{# /.layout-container #}


